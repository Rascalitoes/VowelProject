{

    "manifest_version": 2,
    "name": "Vowels",
    "version": "1.0",
  
    "description": "Randomly changes every vowel to another vowel on a Wikipedia page",
  
    "icons": {
      "48": "icons/border-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.wikipedia.com/*"],
        "js": ["borderify.js"]
      }
    ]
  
  }
  